<!--components/banner/index.wxml-->
<swiper class="banners" indicator-dots="{{true}}" indicator-color indicator-active-color="{{active_dots}}" interval="4000"
        circular autoplay style="height:{{height}}rpx;">
    <block wx:for="{{banners}}" wx:key="index" wx:for-item="item">
        <swiper-item>
            <image src="{{item.img}}" class="item"/>
        </swiper-item>
    </block>
</swiper>