container {
  position: relative;
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.skus {
  padding-left: 30rpx;
  padding-right: 30rpx;
  background-color: #fff;
}

.counter {
  width: 100%;
  background-color: #fff;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.close-c {
  position: absolute;
  right: 10rpx;
  top: 10rpx;
  width:50rpx;
  height:50rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  border-radius: 50%;
}
.close {
  width: 30rpx;
  height: 30rpx;
}

.hasSku {
  padding-right: 30rpx;
  padding-left: 30rpx;
  height: 60rpx;
}

.noSku {
  padding: 30rpx;
}

